
create database if not exists receipts;
use receipts;
create table IF NOT EXISTS QueryReceipts
(
id int not null auto_increment,
area_code varchar(30) not null,
area varchar(100) not null,
date int(8) not null,
pv  int not null,
uv int not null,
primary key (id)
)engine=innodb default charset=utf8;



create table IF NOT EXISTS InquiryReceipts
(
id int not null auto_increment,
area_code varchar(30) not null,
area varchar(100) not null,
date int(8) not null,
pv  int not null,
uv int not null,
primary key (id)
)engine=innodb default charset=utf8;